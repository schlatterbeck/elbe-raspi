<ns0:RootFileSystem xmlns:ns0="https://www.linutronix.de/projects/Elbe" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" created="2009-05-20T08:50:56" revision="6" xsi:schemaLocation="https://www.linutronix.de/projects/Elbe dbsfed.xsd">
    <initvm>
        <buildtype>amd64</buildtype>
        <mirror>
            <primary_host>bee</primary_host>
            <primary_path>/debian</primary_path>
            <primary_proto>http</primary_proto>
            <url-list>
                <url>
                    <binary>
                        http://debian.linutronix.de/elbe-testing wheezy main
                    </binary>
                </url>
            </url-list>
        </mirror>
        <noauth/>
        <suite>wheezy</suite>
    </initvm>
    <project>
        <name>raspberrypi</name>
        <version>1.0</version>
        <description>
            rootfs for raspberrypi
        </description>
        <buildtype>armhf</buildtype>
        <mirror>
            <primary_host>mirrordirector.raspbian.org</primary_host>
            <primary_path>/raspbian</primary_path>
            <primary_proto>http</primary_proto>
            <url-list>
                <url>
                    <binary>
                        http://mirrordirector.raspbian.org/raspbian wheezy main contrib non-free rpi
                    </binary>
                </url>
                <url>
                    <binary>
                        http://archive.raspberrypi.org/debian wheezy main contrib non-free
                    </binary>
                </url>
                <url>
                    <binary>
                        http://debian.linutronix.de/elbe-testing wheezy main
                    </binary>
                </url>
            </url-list>
        </mirror>
        <noauth />
        <suite>wheezy</suite>
    </project>
    <target>
        <hostname>raspi</hostname>
        <domain>linux.local</domain>
        <passwd>xyzzy</passwd>
        <console>ttyAMA0,115200</console>
        <images>
            <msdoshd>
                <name>sdcard.img</name>
                <size>1900MiB</size>
                <partition>
                    <size>56MiB</size>
                    <label>boot</label>
                    <bootable />
                </partition>
                <partition>
                    <size>remain</size>
                    <label>rfs</label>
                </partition>
            </msdoshd>
        </images>
        <fstab>
            <bylabel>
                <label>rfs</label>
                <mountpoint>/</mountpoint>
                <fs>
                    <type>ext2</type>
                    <tune2fs>-i 0</tune2fs>
                </fs>
            </bylabel>
            <bylabel>
                <label>boot</label>
                <mountpoint>/boot</mountpoint>
                <fs>
                    <type>vfat</type>
                </fs>
            </bylabel>
        </fstab>
        <norecommend />
        <finetuning>
            <rm>/var/cache/apt/archives/*.deb</rm>
        </finetuning>
        <pkg-list>
            <pkg>raspberrypi-bootloader</pkg>
            <pkg>raspi-config</pkg>
            <pkg>openssh-server</pkg>
            <pkg>less</pkg>
            <pkg>bash</pkg>
            <pkg>vim-nox</pkg>
            <pkg>wget</pkg>
            <pkg>ntp</pkg>
        </pkg-list>
    </target>
</ns0:RootFileSystem>
