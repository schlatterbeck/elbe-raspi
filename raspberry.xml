<ns0:RootFileSystem xmlns:ns0="https://www.linutronix.de/projects/Elbe" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" created="2009-05-20T08:50:56" revision="6" xsi:schemaLocation="https://www.linutronix.de/projects/Elbe dbsfed.xsd">
    <initvm>
        <buildtype>amd64</buildtype>
        <mirror>
            <primary_host>bee</primary_host>
            <primary_path>/debian</primary_path>
            <primary_proto>http</primary_proto>
            <url-list>
                <url>
                    <binary>
                        http://debian.linutronix.de/elbe stretch main
                    </binary>
		    <key>
			http://debian.linutronix.de/elbe/elbe-repo.pub
		    </key>
                </url>
		<url>
		  <binary>
		    http://debian.linutronix.de/elbe-common stretch main
		  </binary>
		  <key>
		    http://debian.linutronix.de/elbe/elbe-repo.pub
		  </key>
		</url>
            </url-list>
        </mirror>
        <noauth/>
        <suite>stretch</suite>
	<pkg-list>
	  <pkg>openssh-server</pkg>
	</pkg-list>
	<portforwarding>
	  <forward>
	    <proto>tcp</proto>
	    <buildenv>22</buildenv>
	    <host>5022</host>
	  </forward>
	  <forward>
	    <proto>tcp</proto>
	    <buildenv>7588</buildenv>
	    <host>7587</host>
	  </forward>
	</portforwarding>
    </initvm>
    <project>
        <name>raspberrypi</name>
        <version>1.0</version>
        <description>
            rootfs for raspberrypi
        </description>
        <buildtype>armhf</buildtype>
        <mirror>
            <primary_host>mirrordirector.raspbian.org</primary_host>
            <primary_path>/raspbian</primary_path>
            <primary_proto>http</primary_proto>
            <url-list>
                <url>
                    <binary>
                        http://mirrordirector.raspbian.org/raspbian stretch main contrib non-free rpi firmware
                    </binary>
                </url>
                <url>
                    <binary>
                        http://archive.raspberrypi.org/debian/ stretch main
                    </binary>
                </url>
                <url>
                    <binary>
                        http://debian.linutronix.de/elbe stretch main
                    </binary>
                </url>
            </url-list>
        </mirror>
        <noauth />
        <suite>stretch</suite>
    </project>
    <target>
        <hostname>raspi</hostname>
        <domain>linux.local</domain>
        <passwd>xyzzy</passwd>
        <console>ttyAMA0,115200</console>
        <images>
            <msdoshd>
                <name>sdcard.img</name>
                <size>1900MiB</size>
                <partition>
                    <size>56MiB</size>
                    <label>boot</label>
                    <bootable />
                </partition>
                <partition>
                    <size>remain</size>
                    <label>rfs</label>
                </partition>
            </msdoshd>
        </images>
        <fstab>
            <bylabel>
                <label>rfs</label>
                <mountpoint>/</mountpoint>
                <fs>
                    <type>ext4</type>
                    <tune2fs>-i 0</tune2fs>
                </fs>
            </bylabel>
            <bylabel>
                <label>boot</label>
                <mountpoint>/boot</mountpoint>
                <fs>
                    <type>vfat</type>
                </fs>
            </bylabel>
        </fstab>
        <norecommend />
        <finetuning>
            <rm>/var/cache/apt/archives/*.deb</rm>
            <rm>/etc/udev/rules.d/70-persistent-net.rules</rm>
            <command>/bin/gencfg</command>
        </finetuning>
        <pkg-list>
            <pkg>raspberrypi-bootloader-nokernel</pkg>
            <pkg>linux-image-rpi</pkg>
            <pkg>libraspberrypi-bin</pkg>
            <pkg>raspi-config</pkg>
            <pkg>openssh-server</pkg>
            <pkg>less</pkg>
            <pkg>bash</pkg>
            <pkg>vim-nox</pkg>
            <pkg>wget</pkg>
            <pkg>ntp</pkg>
            <pkg>asterisk</pkg>
            <pkg>asterisk-dahdi</pkg>
            <pkg>asterisk-mobile</pkg>
            <pkg>asterisk-prompt-de</pkg>
            <pkg>asterisk-voicemail</pkg>
            <pkg>apache2</pkg>
            <pkg>madplay</pkg>
            <pkg>fake-hwclock</pkg>
            <pkg>openvpn</pkg>
            <pkg>git</pkg>
            <pkg>bluez</pkg>
            <pkg>bind9</pkg>
            <pkg>host</pkg>
            <pkg>isc-dhcp-server</pkg>
        </pkg-list>
    </target>
</ns0:RootFileSystem>
